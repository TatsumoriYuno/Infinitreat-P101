--called whenever we want user to port to quest (whether on foot or ship)
function port_to_quest()
   port_to_quest_foot()

   --we will only port ship if it is currently in use...
   if (getAddressList().getMemoryRecordByDescription('ship max speed ptr').value ~= nil) then
       port_to_quest_shipMode()
   end
end

