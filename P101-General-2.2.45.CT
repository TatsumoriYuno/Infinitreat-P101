 local player_x_addr = getAddressList().getMemoryRecordByDescription('X').CurrentAddress
local player_y_addr = getAddressList().getMemoryRecordByDescription('Y').CurrentAddress
local player_z_addr = getAddressList().getMemoryRecordByDescription('Z').CurrentAddress

local player_facingDir_addr = getAddressList().getMemoryRecordByDescription('Facing (Horizontal)').CurrentAddress

-- store player's current position
local curr_x = readFloat(player_x_addr)
local curr_y = readFloat(player_y_addr)
local curr_z = readFloat(player_z_addr)


writeFloat(player_facingDir_addr, 2.420498848)

writeFloat(player_x_addr, 268.4401855)
writeFloat(player_y_addr, -2337.750977)
writeFloat(player_z_addr, 1168.513428)

timer = createTimer()
timer.interval = 2000 --200 ms delay
timer.onTimer = function ()
writeFloat(player_x_addr, curr_x)
writeFloat(player_y_addr, curr_y)
writeFloat(player_z_addr, curr_z)
timer.destroy()
print("timer destroyed")
end


