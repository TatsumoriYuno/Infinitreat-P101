KILL_TIMER = false

local shouldAlternate = false --related to facing direction math

function listen_for_user_input()
    local didPlayerInput = false --if player inputted, this will be true

    local player_x = getAddressList().getMemoryRecordByDescription('X')
    local player_y = getAddressList().getMemoryRecordByDescription('Y')
    local player_z = getAddressList().getMemoryRecordByDescription('Z')


    local increment_val = 20.0

--move on x axis controls
    if(isKeyPressed(VK_5)) then
        writeFloat(player_x.CurrentAddress, player_x.value + increment_val)
        didPlayerInput = true
    end
    if(isKeyPressed(VK_6)) then
        writeFloat(player_x.CurrentAddress, player_x.value - increment_val)
        didPlayerInput = true
    end

--move on y axis controls
    if(isKeyPressed(VK_8)) then
        writeFloat(player_y.CurrentAddress, player_y.value + increment_val)
        didPlayerInput = true
    end
    if(isKeyPressed(VK_7)) then
        writeFloat(player_y.CurrentAddress, player_y.value - increment_val)
        didPlayerInput = true
    end

--move on z axis controls
    if(isKeyPressed(VK_9)) then
        writeFloat(player_z.CurrentAddress, player_z.value + increment_val)
        didPlayerInput = true
    end
    if(isKeyPressed(VK_0)) then
        writeFloat(player_z.CurrentAddress, player_z.value - increment_val)
        didPlayerInput = true
    end
--move player IFF playe inputted something...
    if (didPlayerInput == true) then
        local player_movement_addr = getAddressList().getMemoryRecordByID(5996).CurrentAddress --player movement address

        if(shouldAlternate) then
            writeInteger(player_movement_addr, 32) --update player camera by moving after teleport
        else
            --writeInteger(player_movement_addr, 64)
        end
        delay_then_reset_player_movement(player_movement_addr)
        shouldAlternate = not shouldAlternate

    end --end of player movement if statement

end --end of listen_to_user_input function


--helper function to delay an action
function delay_then_reset_player_movement(_player_movement_addr) --function that delays reset of player movement
    timer = createTimer(nil, true)
    timer.Interval = 20 --default was 3000
    timer.OnTimer = function(timer)
        writeInteger(_player_movement_addr, 0)
        timer.destroy()
        --print("Timer destroyed")
    end

end




function enableTimer()
    timer = createTimer(nil, true)
    timer.Interval = 1
    timer.OnTimer = function(timer)
    if KILL_TIMER == true then
        timer.destroy()
        --print("Timer destroyed")
    else
        listen_for_user_input()
        end
    end
end

